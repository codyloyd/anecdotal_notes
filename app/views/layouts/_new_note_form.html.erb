<%= form_for @note do |f| %>
<!-- <%= f.text_field :student_id %> -->
<%= f.text_area :content %><br>
<% if cookies[:selected_student_group_id] %>
<%= f.select :student_id, StudentGroup.find(cookies[:selected_student_group_id]).students.collect{|p| [p.name, p.id]} %>
<% else %>
<%= f.select :student_id, current_user.students.collect{|p| [p.name, p.id]} %>
<% end %>
<%= f.submit "create note" %>
<% end %>
